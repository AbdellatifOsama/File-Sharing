@model IEnumerable<FileEntity>
@{
    Layout = "_Layout";
    ViewData["Title"] = "Search";
}
<div class="my-4">
    <h2 class="text-secondary position-relative preview-line fw-bold">Search Results(@Model.Count())</h2>
</div>
@if (Model.Count() > 0)
{
    @foreach (var item in Model)
    {
        <partial name="PartialViews/_SearchBrowsePartialView" model="@item" />
    }
}
else
{
    <div class="alert alert-warning py-3 px-2">
        Sorry, No Items Found
    </div>
}
